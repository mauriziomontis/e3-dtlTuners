# Control Records for Tuner System
# set of records required to reset to reset to Zero the enable tuner
#
# developed by Maurizio Montis | INFN-LNL


record(seq, "${PREFIX}:EMR-SM-${INDEX}:EnRst") {
	field(DESC, "Reset to Zero Enable Tuner")
	field(SCAN, "1 second")
	field(SDIS, "DTL:EMR-SM:EnCmd")
	field(DISV, "1")
	field(DO1, "0")
	field(LNK1, "${PREFIX}:EMR-SM-${INDEX}.CNEN")
	field(DO2, "1")
	field(LNK2, "${PREFIX}:EMR-SM-${INDEX}.PROC")
	field(DLY, "1")
}
